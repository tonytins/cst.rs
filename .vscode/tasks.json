{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "process",
      "command": "cargo",
      "problemMatcher": ["$rustc"],
      "group": "build",
      "args": ["build"],
      "label": "Rust: cargo build"
    },
    {
      "type": "process",
      "command": "cargo",
      "problemMatcher": ["$rustc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "args": ["check"],
      "label": "Rust: cargo check"
    },
    {
      "type": "process",
      "command": "cargo",
      "problemMatcher": ["$rustc"],
      "args": ["test"],
      "label": "Rust: cargo test"
    },
    {
      "type": "process",
      "command": "cargo",
      "args": ["test", "--doc"],
      "problemMatcher": ["$rustc"],
      "label": "Rust: cargo test --doc"
    },
    {
      "type": "process",
      "command": "cargo",
      "problemMatcher": ["$rustc"],
      "args": ["clean"],
      "label": "Rust: cargo clean"
    },
    {
      "type": "process",
      "command": "cargo",
      "problemMatcher": ["$rustc"],
      "args": ["run", "--example", "uitext"],
      "label": "Rust: cargo run --example uitext"
    },
    {
      "type": "process",
      "command": "cargo",
      "problemMatcher": ["$rustc"],
      "args": ["run", "--example", "entry"],
      "label": "Rust: cargo run --example entry"
    }
  ]
}
